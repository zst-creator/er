#include <iostream>
#include <string>

std::string concatenateStrings(const std::string& str1, const std::string& str2) {
    return str1 + str2;
}

int main() {
    std::string s1, s2;
    std::cout << "请输入第一个字符串: ";
    std::cin >> s1;
    std::cout << "请输入第二个字符串: ";
    std::cin >> s2;

    std::string result = concatenateStrings(s1, s2);
    std::cout << "连接后的字符串为: " << result << std::endl;


    return 0;
}
